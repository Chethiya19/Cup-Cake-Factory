<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pinkwall"
    tools:context=".Home_Page">

  <Button
      android:id="@+id/btnmemberlog"
      android:layout_width="255dp"
      android:layout_height="58dp"
      android:layout_marginTop="200dp"
      android:layout_marginEnd="76dp"
      android:background="@drawable/btn_bg"
      android:drawableLeft="@drawable/ic_baseline_person_24"
      android:drawablePadding="0sp"
      android:onClick="MemberLog"
      android:text="Member Login"
      android:textSize="16sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <Button
      android:id="@+id/btnadminlog"
      android:layout_width="255dp"
      android:layout_height="58dp"
      android:layout_marginTop="100dp"
      android:layout_marginEnd="76dp"
      android:background="@drawable/btn_bg"
      android:drawableLeft="@drawable/ic_baseline_admin_panel_settings_24"
      android:drawablePadding="0sp"
      android:onClick="AdminLog"
      android:text="Admin Login"
      android:textSize="16sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btnmemberlog" />

  <Button
      android:id="@+id/btncontactus"
      android:layout_width="255dp"
      android:layout_height="58dp"
      android:layout_marginTop="100dp"
      android:layout_marginEnd="76dp"
      android:background="@drawable/btn_bg"
      android:drawableLeft="@drawable/ic_baseline_perm_contact_calendar_24"
      android:drawablePadding="0sp"
      android:onClick="ContactUs"
      android:text="Contact Us"
      android:textSize="16sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/btnadminlog" />


</android.support.constraint.ConstraintLayout>